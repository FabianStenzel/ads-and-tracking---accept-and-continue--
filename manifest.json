{
    "manifest_version": 3,
    "name": "Cookie & Ad Tracker Overlay",
    "version": "1.0",
    "description": "Extract from the extractor",
    "permissions": [
      "cookies",
      "tabs",
      "storage",
      "scripting",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["overlay.css"]
      }
    ]
  }
  